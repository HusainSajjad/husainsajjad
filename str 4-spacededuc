#include <stdio.h>
#include <string.h>
int main()
{
    int i,j,n;
    char a[75];
    printf("Enter a string:");
    scanf("%[^\n]s",a);
    n=strlen(a);
    for(i=0;i<n;i++)
    {
        if(a[i]==' '){
        for(j=i;j<n;j++)    
        a[j]=a[j+1];
            }
}
printf("The new string is %s",a);
}
