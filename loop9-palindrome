#include<stdio.h>
main()
{
    int n,a,r,s=0;
    printf("\n  Enter The Number:");
    scanf("%d",&n);
    a=n;
    do
    {
        r=n%10;
        s=s*10+r;
        n=n/10;
    }while(n>0);
    if(a==s)
    printf("\n  %d is a Palindrome ",a);
    else
    printf("\n  %d is a not a Palindrome ",a);
}
